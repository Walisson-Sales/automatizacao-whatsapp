<!-- Configurações do n8n-->
N8N_BASIC_AUTH_USER=seu_email@exemplo.com
N8N_BASIC_AUTH_PASSWORD=sua_senha_segura
<!-- Configurações do evolution-api -->
AUTHENTICATION_API_KEY=sua_API_Key_Global
<!-- configuração do banco de dados redis -->
N8N_REDIS_HOST=redis 
N8N_REDIS_PORT=6379
REDIS_PASSWORD=senha_do_redis
<!-- configuração do cache redis -->
CACHE_REDIS_ENABLED=true
CACHE_REDIS_URI=redis://:senha_do_redis@redis:6379/0
CACHE_REDIS_PREFIX_KEY=evolution_v2